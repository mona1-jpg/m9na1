{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["eslint-config-airbnb-base"],
  "rules": {
    "max-len": "off",
    "semi": ["error", "never"],
    "comma-dangle": 0,
    "no-console": "off",
    "object-curly-newline": "off",
    "camelcase": "off",
    "arrow-parens": "off",
    "function-paren-newline": "off",
    "no-case-declarations": "off"
  }
}
